+POST_STRIP_TARGET = nzbhydra_extra_install
+
+# Pure Python package, make sure ARCH is not defined
+override ARCH=
+
+include ../../mk/spksrc.spk.mk
+
+.PHONY: nzbhydra_extra_install
+nzbhydra_extra_install: $(STAGING_DIR)/share/NZBHydra
