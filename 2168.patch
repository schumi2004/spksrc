+RELOAD_UI = yes
+DISPLAY_NAME = NZBHydra
+BETA = 1
+CHANGELOG = "First release"
+
+HOMEPAGE   = https://github.com/theotherp/nzbhydra
+LICENSE    = ApacheV2
